set(O3D3XX_IMAGE_VERSION "@O3D3XX_VERSION_STRING@")

if("${O3D3XX_IMAGE_VERSION}"
    VERSION_LESS "${O3D3XX_IMAGE_FIND_VERSION}")
  set(O3D3XX_IMAGE_VERSION_COMPATIBLE FALSE)
  set(O3D3XX_IMAGE_VERSION_EXACT FALSE)
else()
  set(O3D3XX_IMAGE_VERSION_COMPATIBLE TRUE)
  if ("${O3D3XX_IMAGE_VERSION}"
      VERSION_EQUAL "${O3D3XX_IMAGE_FIND_VERSION}")
    set(O3D3XX_IMAGE_VERSION_EXACT TRUE)
  else()
    set(O3D3XX_IMAGE_VERSION_EXACT FALSE)
  endif()
endif()
